<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="bookBean" class="com.mitrais.gundala.Book">
        <property name="name" value="Menuju TPA Wangi"></property>
    </bean>

    <bean id="bookBean2" class="com.mitrais.gundala.Book">
        <constructor-arg value="Anip yang Tertukar"></constructor-arg>
    </bean>
</beans>